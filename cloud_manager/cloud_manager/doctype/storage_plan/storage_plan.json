{
    "doctype": "DocType",
    "name": "Storage Plan",
    "module": "Cloud Manager",
    "autoname": "format:PLAN-{#####}",
    "engine": "InnoDB",
    "index_web_pages_for_search": 1,
    "sort_field": "modified",
    "sort_order": "DESC",
    "field_order": [
        "customer_id",
        "plan_name",
        "storage_size",
        "price",
        "status"
    ],
    "fields": [
        {
            "label": "Customer",
            "fieldname": "customer_id",
            "fieldtype": "Link",
            "options": "Cloud Customer",
            "reqd": 1
        },
        {
            "label": "Plan Name",
            "fieldname": "plan_name",
            "fieldtype": "Data",
            "reqd": 1
        },
        {
            "label": "Storage Size (GB)",
            "fieldname": "storage_size",
            "fieldtype": "Int",
            "reqd": 1
        },
        {
            "label": "Monthly Cost",
            "fieldname": "price",
            "fieldtype": "Float",
            "reqd": 1
        },
        {
            "label": "Status",
            "fieldname": "status",
            "fieldtype": "Select",
            "options": "Active\nCancelled",
            "default": "Active"
        }
    ],
    "permissions": [
        {
            "role": "Cloud Admin",
            "permlevel": 0,
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 1,
            "submit": 1,
            "cancel": 1,
            "amend": 1,
            "email": 1,
            "print": 1,
            "export": 1,
            "share": 1,
            "report": 1
        },
        {
            "role": "Cloud Staff",
            "permlevel": 0,
            "read": 1,
            "write": 1,
            "create": 1,
            "email": 1,
            "print": 1,
            "export": 1
        },
        {
            "role": "Customer",
            "permlevel": 1,
            "read": 1,
            "user_permission_applies": 1,
            "email": 1,
            "print": 1
        }
    ],
    "actions": [],
    "links": [],
    "states": []
}